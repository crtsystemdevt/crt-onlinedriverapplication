                    <label for="" name="sex">Sex</label> <label class="required">*</label>
                    <select class="form-select text_box" name="sex">
                        <option value="male" name="Male">Male</option>
                        <option value="female" name="Female">Female</option>
                    </select>
                    <label for="">Home Address</label> <label class="required">*</label>
                    <input type="text" class="form-control text_box requiredInput" maxlength="100" oninput="this.className='form-control text_box requiredInput'" id="" placeholder="Home Address" name="homeAddress" oncopy="return false" onpaste="return false">
                    
                    <label for="">Birthdate</label> <label class="required">*</label>
                    <input type="date" class="form-control text_box requiredInput" oninput="this.className='form-control text_box requiredInput'" id="birthdate" placeholder="Birthdate" name="birthdate" oncopy="return false" onpaste="return false" onkeypress="return isName(event)">

                    <label for="" name="location">Location/Area</label> <label class="required">*</label>
                    <select class="form-select text_box" name="location">
                        <option value="Metro Manila" name="Metro Manila">Metro Manila</option>
                        <option value="Laguna" name="Laguna">Laguna</option>
                    </select> <label for=""> <a data-toggle="modal" data-target="#exampleModal" style="color: blue; text-decoration: underline;">Can't find your location? <strong>&#9432</strong></a></label> <br>
                    
                </div>
                
                <div class="col-lg-6">
                    <label for="">Personal contact number</label> <label class="required">*</label>
                    <input type="text" class="form-control text_box requiredInput" maxlength="11" oninput="this.className='form-control text_box requiredInput'" oncopy="return false" onpaste="return false"  onkeypress="return isNumber(event)" pattern="^[0-9]*$" placeholder="Personal contact number" name="personalNumber" oncopy="return false" onpaste="return false">
                    <label for="">Contact person in case of emergency</label> <label class="required">*</label>
                    <input type="text" class="form-control text_box requiredInput" maxlength="100" oninput="this.className='form-control text_box requiredInput'" id="" placeholder="Contact person in case of emergency" name="contactPerson_Name" onkeypress="return isName(event)" oncopy="return false" onpaste="return false">
                    
