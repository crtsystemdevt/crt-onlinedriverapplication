                    <label for="">Blood type</label>
                    <input type="text" class="form-control text_box" id="" maxlength="100" placeholder="Blood type" name="bloodType">
                    <label for="">Religion</label>
                    <input type="text" class="form-control text_box" id="" maxlength="100" placeholder="Religion" name="religion">
                    <label for="">Civil status</label> <label class="required">*</label>
                    <input type="text" class="form-control text_box requiredInput" maxlength="100" oninput="this.className='form-control text_box requiredInput'" id="" placeholder="Civil status" name="civilStatus" onkeypress="return isName(event)" oncopy="return false" onpaste="return false">
